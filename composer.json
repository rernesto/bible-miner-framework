{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "alcaeus/mongo-php-adapter": "^1.1",
        "api-platform/core": "^2.4",
        "doctrine/dbal": "^2.9",
        "doctrine/mongodb-odm-bundle": "4.*",
        "league/csv": "^9.2",
        "markrogoyski/math-php": "^0.51.0",
        "nelmio/cors-bundle": "^1.5",
        "php-ai/php-ml": "^0.8.0",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "4.3.*",
        "symfony/console": "4.3.*",
        "symfony/dotenv": "4.3.*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "4.3.*",
        "symfony/monolog-bundle": "^3.3",
        "symfony/security-bundle": "4.3.*",
        "symfony/twig-bundle": "4.3.*",
        "symfony/yaml": "4.3.*",
        "wamania/php-stemmer": "^1.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "4.3.*"
        }
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.3",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/phpunit-bridge": "4.3.*",
        "symfony/web-server-bundle": "4.3.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
